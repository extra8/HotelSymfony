<form method="post" action="{{ path('feedback_delete', {'Id': feedback.Id}) }}" onsubmit="return confirm('Sigur stergeti recenzia?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ feedback.Id) }}">
    <button class="btn">Sterge</button>
</form>
